{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"src/**/*.d.ts",
		"src/**/*.ts",
		"src/**/*.js",
		"**/*"
	],
	"exclude": ["dist"],
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"]
		}
	}
}
